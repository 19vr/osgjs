{
    "quotmark": "single",
    "bitwise": false,
    "camelcase": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": true,
    "newcap": false,
    "noarg": true,
    "undef": true,
    "unused": true,
    "trailing": true,
    "esnext": true,

    "eqnull": true,
    "laxcomma": true,
    "sub": true,

    "browser": true,
    "node": true,
    "devel": true,

    "predef": [ "-Object", "-Node", "-Image", "-Map", "-Text" ],
    "globals": {
        "define": false,
        "require": false,
        "ok": false,
        "module": false,
        "expect": false,
        "test": false,
        "asyncTest": false,
        "equal": false,
        "start": false,
        "deepEqual": false,
        "__dirname": false,
        "Object": true,
        "Node": true,
        "Image": true,
        "Map": true,
        "Text": true
    }
}
